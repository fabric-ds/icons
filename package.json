{
  "name": "@fabric-ds/icons",
  "type": "module",
  "version": "0.4.2",
  "description": "Fabric's icon set",
  "main": "index.js",
  "files": [
    "dist",
    "vue",
    "react",
    "elements"
  ],
  "exports": {
    ".": "./index.js",
    "./vue": "./vue/index.js",
    "./react": "./react/index.js",
    "./elements": "./elements/index.js",
    "./elements/*": "./elements/*.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "vue": "rimraf vue && npm run build:vue",
    "react": "rimraf react && npm run build:react",
    "elements": "rimraf elements && npm run build:elements",
    "prepublishOnly": "rimraf dist && npm run build && npm run vue && npm run react && npm run elements",
    "import": "node ./scripts/figma-import.js",
    "build": "node ./scripts/build.js",
    "build:vue": "mkdir vue && node ./generateVue.js && node ./vue.eik.js",
    "build:react": "mkdir react && node ./generateReact.js && node ./react.eik.js",
    "build:elements": "mkdir elements && node ./generateElements.js && node ./elements.eik.js",
    "render": "node preview/render.js",
    "start": "npm run build && node preview/render.js && open ./preview/index.html",
    "version": "npm publish",
    "postversion": "git push --follow-tags",
    "update-lib": "npm run import && npm run build && npm run render && cp ./preview/index.html ./docs/index.html"
  },
  "license": "MIT",
  "devDependencies": {
    "@eik/esbuild-plugin": "^1.1.11",
    "@sindresorhus/slugify": "^1.1.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.0",
    "esbuild": "^0.14.42",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.7",
    "jsdom": "^16.5.3",
    "nanoid": "^3.1.20",
    "node-fetch": "^2.6.1",
    "nunjucks": "^3.2.2",
    "ora": "^5.2.0",
    "prompts": "^2.4.0",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "eik": {
    "server": "https://assets.finn.no",
    "type": "package",
    "files": "./dist",
    "import-map": [
      "https://assets.finn.no/map/react/v2",
      "https://assets.finn.no/map/vue/v3",
      "https://assets.finn.no/map/custom-elements/v1"
    ]
  }
}
