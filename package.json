{
  "name": "@finn-no/fabric-icons",
  "version": "0.3.7",
  "description": "Fabric's icon set",
  "main": "index.js",
  "files": [
    "dist",
    "vue"
  ],
  "exports": {
    ".": "./index.js",
    "./vue": "./vue/index.js"
  },
  "scripts": {
    "vue": "rimraf vue && npm run build:vue",
    "prepublishOnly": "rimraf dist && npm run build && npm run vue",
    "build": "node ./scripts/build.js",
    "build:vue": "mkdir vue && node ./generateVue.mjs",
    "start": "npm run build && node preview/render.js && open ./preview/index.html",
    "version": "npm publish",
    "postversion": "git push --follow-tags"
  },
  "license": "MIT",
  "devDependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.7",
    "jsdom": "^16.5.3",
    "nanoid": "^3.1.20",
    "node-fetch": "^2.6.1",
    "nunjucks": "^3.2.2",
    "ora": "^5.2.0",
    "prompts": "^2.4.0",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
